    "            home_button1 = gr.Button(\"ğŸ  í™ˆìœ¼ë¡œ\", elem_id=\"home-button\")\n",
    "        \n",
    "        upload_button = gr.Button(\"Image Upload\", elem_id=\"image-upload-area\")\n",
    "        gender = gr.Radio([\"ë‚¨ì\", \"ì—¬ì\"], label=\"ì„±ë³„\")\n",
    "        height = gr.Textbox(label=\"ì‹ ì¥ (cm)\", placeholder=\"ì˜ˆ: 180\")\n",
    "        weight = gr.Textbox(label=\"ì²´ì¤‘ (kg)\", placeholder=\"ì˜ˆ: 70\")\n",
    "        age = gr.Textbox(label=\"ë‚˜ì´ (ì„¸)\", placeholder=\"ì˜ˆ: 25\")\n",
    "        activity_level = gr.Radio([\"ë‚®ìŒ\", \"ë³´í†µ\", \"ë†’ìŒ\", \"ì‹¬í•¨\"], label=\"í™œë™ ìˆ˜ì¤€\", \n",
    "                                  info=\"ìœ¡ì²´í™œë™ì´ ê±°ì˜ ì—†ëŠ” ê²½ìš° (ë‚®ìŒ), ë³´í†µì˜ í™œë™ (ë³´í†µ), ì‹¬í•œ í™œë™ (ë†’ìŒ), ë§¤ìš° ì‹¬í•œ í™œë™ (ì‹¬í•¨)\")\n",
    "\n",
    "        recommended_intake_output = gr.Textbox(label=\"ê¶Œì¥ ì„­ì·¨ëŸ‰\", interactive=False)\n",
    "        calculate_button = gr.Button(\"ê¶Œì¥ ì„­ì·¨ëŸ‰ ê³„ì‚°\")\n",
    "\n",
    "        calculate_button.click(\n",
