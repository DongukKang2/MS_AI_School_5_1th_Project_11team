    "        pie_chart = gr.Plot()\n",
    "        recommendation_output = gr.Markdown()\n",
    "\n",
    "        # ì§€ë„ ë§í¬ ë²„íŠ¼ì„ HTML ë§í¬ë¡œ ë³€ê²½\n",
    "        map_button = gr.HTML(\"<a href='http://3.39.174.171/' target='_blank' style='text-decoration:none; color:white;'><button style='background-color:#4CAF50; color:white; padding:10px; font-size:1em; font-weight:bold;'>ì§€ë„ì—ì„œ í™•ì¸í•˜ê¸°</button></a>\")\n",
    "        \n",
    "        back_button = gr.Button(\"ë’¤ë¡œ ê°€ê¸°\")\n",
    "\n",
    "    # ì„¸ ë²ˆì§¸ í™”ë©´ (ì§€ë„ í™”ë©´)\n",
    "    with gr.Column(visible=False) as third_screen:\n",
    "        with gr.Row():\n",
    "            home_button3 = gr.Button(\"ğŸ  í™ˆìœ¼ë¡œ\", elem_id=\"home-button\")\n",
    "            back_button2 = gr.Button(\"ë’¤ë¡œ ê°€ê¸°\")\n",
    "        gr.Markdown(\"<div class='note'>ì´ê³³ì— ì§€ë„ í™”ë©´ì´ í‘œì‹œë©ë‹ˆë‹¤ (ì˜ˆì‹œ)</div>\")\n",
    "\n",
    "    start_button.click(\n",
    "        fn=lambda: (gr.update(visible=False), gr.update(visible=True)),\n",
    "        outputs=[home_screen, first_screen]\n",
